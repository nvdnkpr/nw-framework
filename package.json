{
  "name": "nw-framework",
  "description": "A framework for Node Webkit",
  "version": "0.0.1",
  "main": "./NWMain",
  "dependencies": {
    "async": "0.2.9",
    "datautils": "0.1.0",
    "nwglobal": "0.0.1",
    "underscore": "1.5.2"
  },
  "devDependencies": {
    "chai": "*",
    "istanbul": "*",
    "jshint": "*",
    "mocha": "*"
  },
  "author": "Simon Emms <simon@simonemms.com>",
  "contributors": [
    {
      "name": "Simon Emms",
      "email": "simon@simonemms.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/riggerthegeek/nw-framework.git"
  },
  "bugs": {
    "url": "https://github.com/riggerthegeek/nw-framework/issues"
  },
  "licence": "GPL-3.0",
  "scripts": {
    "test": "jshint ./ && mocha test --colors --reporter spec --recursive"
  },
  "jshintConfig": {
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "node": true,
    "newcap": true,
    "quotmark": "double",
    "trailing": true,
    "undef": true,
    "unused": false
  }
